{
	"name": "hello-gbdx-<your initials>",
	"description": "Writes list of the input file names and a user defined message to output file out.txt.",
	"properties": {
		"isPublic": true,
		"timeout": 7200
	},
	"inputPortDescriptors": [{
		"name": "message",
		"type": "string",
		"description": "User defined message.",
		"required": true
	}, {
		"name": "data_in",
		"type": "directory",
		"description": "Input data directory.",
		"required": true
	}],
	"outputPortDescriptors": [{
		"name": "data_out",
		"type": "directory",
		"description": "Output data directory."
	}],
	"containerDescriptors": [{
		"type": "DOCKER",
		"properties": {
			"image": "egolden/hello-gbdx-docker-image"
		},
		"command": "python /hello-gbdx.py",
		"isPublic": true
	}]
}
